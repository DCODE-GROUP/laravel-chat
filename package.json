{
  "name": "laravel-chat",
  "version": "0.0.1",
  "description": "Frontend vue components for the laravel-chat package",
  "main": ".eslintrc.js",
  "keywords": [
    "vue",
    "tailwind",
    "dsg",
    "untitledui",
    "laravel-chat",
    "dcode"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "npx only-allow pnpm",
    "preupdate": "npx --yes only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "serve": "vite preview",
    "lint": "eslint --flag unstable_ts_config src/",
    "lint:fix": "eslint --flag unstable_ts_config --fix src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DCODE-GROUP/laravel-chat.git"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 9.0.0",
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/laravel-chat.esm.js",
      "require": "./dist/laravel-chat.cjs",
      "default": "./dist/laravel-chat.esm.js"
    },
    "./laravel-chat.css": "./dist/laravel-chat.css"
  },
  "peerDependencies": {
    "vue": "^3.1.0"
  },
  "devDependencies": {
    "@dcodegroup-au/dsg-vue": "^0.0.13-alpha.6"
  }
}
